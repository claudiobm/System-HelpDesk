<h1>Listing account_departments</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
  </tr>

<% @account_departments.each do |account_department| %>
  <tr>
    <td><%=h account_department.name %></td>
    <td><%=h account_department.description %></td>
    <td><%= link_to 'Visualizar', admin_account_department_path(account_department) %></td>
    <td><%= link_to 'Edit', edit_admin_account_department_path(account_department) %></td>
    <td><%= link_to 'Excluir', admin_account_department_path(account_department), :confirm => 'Tem certeza?', :method => :delete %> %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New account_department', new_admin_account_department_path %>
