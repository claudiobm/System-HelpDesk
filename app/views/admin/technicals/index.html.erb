<h1>Listas de técnicos</h1>

<table>
  <tr>
    <th>Nome</th>
    <th>Telefone 1</th>
    <th>Nextel</th>
    <th>Email</th>
    <th>Regions</th>
  </tr>

<% @technicals.each do |technical| %>
  <tr>
    <td><%=h technical.name %></td>
    <td><%=h technical.tel1 %></td>
    <td><%=h technical.nextel %></td>
    <td><%=h technical.email %></td>
    <td><%=h technical.regions %></td>
    <td><%= link_to 'Visualizar', admin_technical_path(technical) %></td>
    <td><%= link_to 'Edit', edit_admin_technical_path(technical) %></td>
    <td><%= link_to 'Excluir', admin_technical_path(technical), :confirm => 'Tem certeza?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Novo técnico', new_admin_technical_path %>
